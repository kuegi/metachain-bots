  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Interact",
      "skipFiles": ["<node_internals>/**"],
      "env": { "NODE_OPTIONS": "--openssl-legacy-provider" },
      "program": "${workspaceFolder}/bots/winnerTakesItAll.ts",
      "args": ["privKey"],
      "preLaunchTask": "npm: build",
      "outFiles": ["${workspaceFolder}/build/**/*.js"]
    }
  ]
}
